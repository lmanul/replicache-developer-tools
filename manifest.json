{
  "manifest_version": 3,
  "name": "Replicache Developer Tools",
  "version": "0.1",
  "description": "Extension to help develop applications with Replicache",
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png",
    "512": "icons/512.png"
  },
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "nativeMessaging",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [{
      "resources": [
        "panel.html",
        "build/install_hook.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "extension_ids": []
    }
  ]
}
